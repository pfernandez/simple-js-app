{
  "name": "simple-js-app",
  "version": "0.0.1",
  "description": "A simple JavaScript starter app",
  "author": "Paul Fernandez",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "javascript"
  ],
  "homepage": "https://github.com/pfernandez/simple-js-app",
  "bugs": {
    "url": "https://github.com/pfernandez/simple-js-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfernandez/simple-js-app.git"
  },
  "main": "server.js",
  "scripts": {
